
[
  {
    "description": "fromE - Switch to Firefox Dev",
    "manipulators": [
      {
        "from": {
          "key_code": "w",
          "modifiers": {
            "mandatory": [
              "shift",
              "command"
            ]
          }
        },
        "to": [
          {
            "key_code": "1",
            "modifiers": [
              "control"
            ]
          },
          {

            "shell_command": "open -a '/Applications/Firefox Developer Edition.app/Contents/MacOS/firefox'"
          }
        ],
        "type": "basic"
      }

    ]
  },

