{
  "description": "Switch to Firefox Space 2 (Swap)",
  "manipulators": [
    {
      "from": {
        "key_code": "d",
        "modifiers": {
          "mandatory": [
            "shift",
            "command"
          ]
        }
      },
      "to": [
        {
          "key_code": "3",
          "modifiers": [
            "control"
          ]
        },
        {
          "key_code": "f10"
        },
        {
          "shell_command": "open -a '/Applications/Firefox.app/Contents/MacOS/firefox'"
        }
      ],
      "type": "basic"
    }
  ]
}
